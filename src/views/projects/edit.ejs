
<div class="edit-container">
  <div class="edit-inner-container">
    <h2>Edit project</h2>
    <form action="/projects/<%= project.id %>?_method=PUT" method="post">
      <%- include("_form_fields.ejs") %>
      <%- include("../partials/validations.ejs") %>
      <div class="update-cancel-container">
        <a class="edit-button cancel" href="/projects/all">Cancel</a>
        <button class="submit-btn" type="submit">Update Project</button>
      </div>
    </form>
  </div>
</div>